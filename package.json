{
  "name": "mississippi-burn-bans-api",
  "module": "src/index.ts",
  "type": "module",
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "scripts": {
    "build": "rm -rf dist/ && bun build --compile --production --outfile dist/mississippi-burn-bans-api src/index.ts",
    "dev": "bun run --hot src/index.ts",
    "start": "bun run src/index.ts",
    "typecheck": "tsc --noEmit"
  }
}
